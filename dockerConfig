docker build . -f Dockerfile -t yemektarifiodev
docker run --rm -it yemektarifiodev
docker-compose down --rmi all
docker kill $(docker ps -q)
docker rm $(docker ps -a -q)
docker network rm yemektarifiodev-mysql

docker run --name mysql-standalone -e MYSQL_ROOT_PASSWORD=1234 -e MYSQL_DATABASE=yemektarifiodev -e MYSQL_PASSWORD=1234 -d mysql



mvn package -Dmaven.test.skip=true




docker network create yemektarifiodev-mysql
docker network ls
docker container run --name mysqldb --network yemektarifiodev-mysql -e MYSQL_ROOT_PASSWORD=1234 -e MYSQL_DATABASE=yemektarifiodev -d mysql:8

docker image build -t yemektarifiodev-jdbc .

docker container run --network yemektarifiodev-mysql --name yemektarifiodev-jdbc-container -p 8080:8080 -d yemektarifiodev-jdbc




